{
  "kind": "collectionType",
  "collectionName": "self-services",
  "info": {
    "singularName": "self-service",
    "pluralName": "self-services",
    "displayName": "Self-service",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category",
      "inversedBy": "self-services"
    },
    "services": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::service.service",
      "inversedBy": "self-services"
    },
    "benefit": {
      "type": "string"
    },
    "information": {
      "type": "string"
    },
    "process": {
      "type": "string"
    },
    "customer": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "qty": {
      "type": "integer"
    },
    "deadline": {
      "type": "integer"
    },
    "description": {
      "type": "json"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    
    "status": {
      "type": "enumeration",
      "enum": [
        "comment",
        "chosen",
        "viewed-job"
      ]
    },
    "files": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
